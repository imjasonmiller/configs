{
    "eslint.filetypes": [
        "javascript",
        "typescript",
        "typescriptreact",
        "javascriptreact"
    ],
    "coc.preferences.formatOnSaveFiletypes": [
        "css", 
        "javascript", 
        "typescript", 
        "html",
        "scss"
